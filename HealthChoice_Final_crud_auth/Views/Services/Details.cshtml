@{
    var Services = (ServicesModel)ViewData["Services"];
}



<div class="card mb-5" style="margin-bottom:20px; margin-top:20px; text-align:center">
    <div class="card mb-6">
        <h3 class="card-header">@Services.servName </h3>
        <div class="card-body">
            <p> @Services.servOverview</p>
            <div class="row" style="align-content: center">
                <div class="col d-flex justify-content-center align-items-center">
                    <img class="img-fluid" src="@Services.servLogo" alt="Card image cap" height="400" width="200">
                </div>
            </div>
        </div>
        <div class="card-body">
            <a href=@Services.servWebsite class="btn btn-dark">Visist</a>

        </div>
        <div class="card-body">
            <div class="card-body">
                <a href="/Services" class="btn btn-dark">Back</a>
            </div>

        </div>
    </div>
</div>








